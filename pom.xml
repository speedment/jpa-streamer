    <issueManagement>
        <system>Github</system>
        <url>https://github.com/speedment/jpa-streamer/issues</url>
    </issueManagement>
    
    <scm>
        <connection>scm:git:git://github.com/speedment/jpa-streamer.git</connection>
        <developerConnection>scm:git:git@github.com:speedment/jpa-streamer.git</developerConnection>
        <url>https://github.com/speedment/jpa-streamer/</url>
