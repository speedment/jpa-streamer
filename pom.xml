    <packaging>pom</packaging>

    <!-- Project Metadata -->
    <name>Parent</name>
    <description>
        JPAstreamer - a lightweight library for expressing JPA queries as Java Streams
    </description>
    
    <!-- Modules -->
    <modules>
        <module>exception</module>
        <module>rootfactory</module>
        <module>field</module>
        <module>application</module>
        <module>javanine</module>
        <module>autoclose</module>
        <module>pipeline</module>
        <module>builder</module>
        <module>renderer</module>
        <module>interopoptimizer</module>
        <module>merger</module>
        <module>criteria</module>
        <module>termopmodifier</module>
        <module>termopoptimizer</module>
        <module>provider</module>
        <module>fieldgenerator</module>
        <module>core</module>
        <module>integration</module>
        <module>analytics</module>
        <module>appinfo</module>
        <module>streamconfiguration</module>
        <module>announcer</module>
        <module>projection</module>
    </modules>
    
    <!-- Properties -->
    <properties>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <project.build.resourceEncoding>UTF-8</project.build.resourceEncoding>
        <project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>

        <maven.compiler.source>1.8</maven.compiler.source>
        <maven.compiler.target>1.8</maven.compiler.target>
        <junit.jupiter.version>5.7.1</junit.jupiter.version>
        <junit.platform.provider.version>1.7.1</junit.platform.provider.version>
        <mockito.version>3.8.0</mockito.version>
        <jpa-streamer.version>${project.version}</jpa-streamer.version>
        <speedment-version>3.2.10</speedment-version>

        <!-- later maven-javadoc-plugin versions does not support aggregate? -->
        <jpa.version>2.2</jpa.version>
        <maven-javadoc-plugin-version>3.2.0</maven-javadoc-plugin-version>
        <maven-source-plugin-version>3.2.1</maven-source-plugin-version>
        <maven-dependency-plugin-version>3.1.2</maven-dependency-plugin-version>
        <maven-shade-plugin-version>3.2.4</maven-shade-plugin-version>
        <jacoco-maven-plugin.version>0.8.6</jacoco-maven-plugin.version>
    </properties>
    
    <organization>
        <name>Speedment, Inc.</name>
        <url>http://www.speedment.com/</url>
    </organization>
    
    <licenses>
        <license>
            <name>LGPL 2.1</name>
